function isPrime(num) {
    if (num == 0 || num == 1) {
        console.log("invalid number");
        return false;
    }

    for (let i = 2; i <= num; i++) {
        if (num % i === 0) {
            console.log("Not prime");
            return false;
        }
        console.log("is prime");
        return true;
    }

}

isPrime(3);
